<%= simple_form_for(@med) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :med_name %>
    <%= f.input :dosage %>
    <%= f.input :amount %>
    <%= f.input :start_date %>
    <%= f.input :customer_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
